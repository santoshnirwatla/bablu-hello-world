pipeline{
  agent any

  stages{
    stage("parallel stage"){
      parallel{
        stage("one"){
          steps{
            sh """
            echo "one"
            """
          }
        }
        stage("two"){
          steps{
            sh """
            echo "two"
            """
          }
        }
        stage("third"){
          steps{
            sh """
            echo "third"
            """
          }
        }
      }
    }
    stage("squentail"){
      steps{
         sh """
          echo "sequentaails"
         """
      }
    }
  }
}